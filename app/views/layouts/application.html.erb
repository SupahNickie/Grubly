<!DOCTYPE html>
<html>
<% flash.each do |key, value| %>
  <div>
    <%= value %>
  </div>
<% end %>

<%= render 'layouts/head' %>
<%= render 'layouts/headernav' %>

<% if current_user %>
  <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %><br>
<% else %>
  <%= link_to 'Sign In', new_user_session_path %><br>
  <%= link_to 'Sign Up', new_user_registration_path %><br>
  <%= link_to "Login with Twitter", user_omniauth_authorize_path(:twitter) %><br>
<% end %>

<div class="container">

  <%= yield %>

</div>


</html>
